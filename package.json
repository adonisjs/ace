{
  "name": "@adonisjs/ace",
  "version": "5.0.1",
  "description": "Ace is a command line tool for adonisjs framework",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "posttest": "npm run coverage",
    "test:local": "FORCE_COLOR=true node bin/index.js --local",
    "test": "nyc npm run test:local",
    "test:win": "set FORCE_COLOR=true && node bin/index.js --win",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "adonisjs",
  "license": "MIT",
  "devDependencies": {
    "clear-module": "^2.1.0",
    "coveralls": "^3.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "japa": "^1.0.6",
    "japa-cli": "^1.0.1",
    "nyc": "^11.4.1",
    "semver": "^5.5.0",
    "standard": "^11.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "chalk": "^2.3.2",
    "cli-table": "^0.3.1",
    "commander": "^2.14.1",
    "debug": "^3.1.0",
    "enquirer": "^1.0.3",
    "fs-extra": "^5.0.0",
    "is-arrow-function": "^2.0.3",
    "lodash": "^4.17.5",
    "mustache": "^2.3.0",
    "prompt-checkbox": "^2.2.0",
    "prompt-confirm": "^1.2.0",
    "prompt-expand": "^1.0.0",
    "prompt-list": "^3.1.2",
    "prompt-password": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/ace.git"
  },
  "keywords": [
    "cli",
    "adonisjs",
    "adonis",
    "terminal",
    "adonis-ace"
  ],
  "bugs": {
    "url": "https://github.com/adonisjs/ace/issues"
  },
  "homepage": "https://github.com/adonisjs/ace#readme",
  "bin": {
    "ada": "examples/index.js"
  },
  "standard": {
    "ignore": "examples/"
  },
  "nyc": {
    "exclude": [
      "bin",
      "src/Question",
      "test"
    ]
  }
}
