{
  "name": "@adonisjs/ace",
  "version": "5.0.4",
  "description": "Ace is a command line tool for adonisjs framework",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "posttest": "npm run coverage",
    "test:local": "FORCE_COLOR=true node bin/index.js --local",
    "test": "nyc japa",
    "test:win": "node ./node_modules/japa-cli/index.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "commit": "git-cz",
    "lint": "standard"
  },
  "author": "adonisjs",
  "license": "MIT",
  "devDependencies": {
    "@adonisjs/mrm-preset": "^1.0.11",
    "clear-module": "^3.0.0",
    "commitizen": "^2.10.1",
    "coveralls": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "japa": "^1.0.6",
    "japa-cli": "^1.0.1",
    "mrm": "^1.2.1",
    "nyc": "^13.0.1",
    "pkg-ok": "^2.2.0",
    "semver": "^5.5.1",
    "standard": "^12.0.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "cli-table": "^0.3.1",
    "commander": "^2.17.1",
    "debug": "^3.1.0",
    "enquirer": "^1.0.3",
    "fast-levenshtein": "^2.0.6",
    "fs-extra": "^7.0.0",
    "is-arrow-function": "^2.0.3",
    "kleur": "^2.0.2",
    "lodash": "^4.17.10",
    "mustache": "^2.3.2",
    "prompt-checkbox": "^2.2.0",
    "prompt-confirm": "^2.0.4",
    "prompt-expand": "^1.0.1",
    "prompt-list": "^3.2.0",
    "prompt-password": "^1.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/ace.git"
  },
  "keywords": [
    "cli",
    "adonisjs",
    "adonis",
    "terminal",
    "adonis-ace"
  ],
  "bugs": {
    "url": "https://github.com/adonisjs/ace/issues"
  },
  "homepage": "https://github.com/adonisjs/ace#readme",
  "bin": {
    "ada": "examples/index.js"
  },
  "standard": {
    "ignore": "examples/"
  },
  "nyc": {
    "exclude": [
      "test",
      "src/Question/index.js",
      "japaFile.js"
    ]
  }
}
