{
  "name": "@adonisjs/ace",
  "version": "4.0.5",
  "description": "Ace is a command line tool for adonisjs framework",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "lint": "standard",
    "pretest": "npm run lint",
    "posttest": "npm run coverage",
    "test:local": "FORCE_COLOR=true node bin/index.js --local",
    "test": "nyc npm run test:local",
    "test:win": "set FORCE_COLOR=true && node bin/index.js --win",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "author": "adonisjs",
  "license": "MIT",
  "devDependencies": {
    "coveralls": "^2.13.1",
    "cz-conventional-changelog": "^2.0.0",
    "japa": "^1.0.3",
    "japa-cli": "^1.0.1",
    "nyc": "^11.0.3",
    "semver": "^5.3.0",
    "sinon": "^2.2.0",
    "standard": "^10.0.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "cli-table": "^0.3.1",
    "commander": "^2.9.0",
    "debug": "^2.6.6",
    "fs-extra": "^4.0.2",
    "inquirer": "^3.0.6",
    "is-arrow-function": "^2.0.3",
    "lodash": "^4.17.4",
    "mustache": "^2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/ace.git"
  },
  "keywords": [
    "cli",
    "adonisjs",
    "adonis",
    "terminal",
    "adonis-ace"
  ],
  "bugs": {
    "url": "https://github.com/adonisjs/ace/issues"
  },
  "homepage": "https://github.com/adonisjs/ace#readme",
  "bin": {
    "ada": "examples/index.js"
  },
  "nyc": {
    "exclude": ["bin", "src/Question", "test"]
  }
}
